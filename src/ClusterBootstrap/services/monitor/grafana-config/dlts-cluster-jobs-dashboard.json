                  "rgba(50, 172, 45, 0.97)"
                ],
                "dateFormat": "YYYY-MM-DD HH:mm:ss",
                "decimals": 2,
                "link": true,
                "linkTargetBlank": true,
                "linkTooltip": "Go to job Statistics for $__cell_1",
                "linkUrl": "/dashboard/db/job-statistics?orgId=1&var-job_name=$__cell_1",
                "pattern": "job_name",
                "sanitize": false,
                "thresholds": [],
                "type": "string",
                "unit": "short"
              },
              {
                "alias": "User",
                "colorMode": null,
                "colors": [
                  "rgba(245, 54, 54, 0.9)",
                  "rgba(237, 129, 40, 0.89)",
                  "rgba(50, 172, 45, 0.97)"
                ],
                "dateFormat": "YYYY-MM-DD HH:mm:ss",
                "decimals": 2,
                "link": true,
                "linkTargetBlank": true,
                "linkTooltip": "Go to User Statistics for $__cell_2 in VC $__cell_3",
                "linkUrl": "/dashboard/db/user-statistics?orgId=1&var-username=$__cell_2&var-vc_name=$__cell_3",
                "pattern": "username",
                "thresholds": [],
                "type": "string",
                "unit": "short"
              },
              {
                "alias": "VC",
                "colorMode": null,
                "colors": [
                  "rgba(245, 54, 54, 0.9)",
                  "rgba(237, 129, 40, 0.89)",
                  "rgba(50, 172, 45, 0.97)"
                ],
                "dateFormat": "YYYY-MM-DD HH:mm:ss",
                "decimals": 2,
                "link": true,
                "linkTargetBlank": true,
                "linkTooltip": "Go to VC Statistics for VC $__cell_3",
                "linkUrl": "/dashboard/db/vc-statistics?refresh=1m&orgId=1&var-vc_name=$__cell_3",
                "pattern": "vc_name",
                "thresholds": [],
                "type": "string",
                "unit": "short"
              },
              {
                "alias": "",
                "colorMode": "value",
                "colors": [
                  "rgba(245, 54, 54, 0.9)",
                  "rgba(237, 129, 40, 0.89)",
                  "rgba(50, 172, 45, 0.97)"
                ],
                "decimals": 2,
                "pattern": "/.*/",
                "thresholds": [
                  "50",
                  "90"
                ],
                "type": "number",
                "unit": "percent"
              }
            ],
            "targets": [
              {
                "expr": "avg(task_gpu_percent) by (job_name, username, vc_name)",
                "format": "table",
                "hide": false,
                "instant": true,
                "intervalFactor": 2,
                "legendFormat": "",
                "refId": "A"
              }
            ],
            "title": "Current Assigned GPU Utilization",
            "transform": "table",
            "type": "table"
          }
