{
  "manifest_version": 2,
  "name": "DLTS Dashboard Helper",
  "version": "0.0.0",

  "description": "Web extension for DLTS dashboard.",
  "icons": {
    "128": "icon.png"
  },

  "browser_action": {
    "default_popup": "dist/popup.html"
  },

  "permissions": [
    "activeTab",
    "storage"
  ],
  "optional_permissions": [
    "cookies",
    "*://*/*"
  ],

  "options_page": "dist/options.html"
}
